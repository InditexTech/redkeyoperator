{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Redkey Manager",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/main.go"
        },
        {
            "name": "Connect to Manager",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 40000,
            "host": "127.0.0.1",
            "trace": "verbose"
        }
    ]
}
